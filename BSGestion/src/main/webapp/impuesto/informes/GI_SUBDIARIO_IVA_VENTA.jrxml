	<queryString>
		<![CDATA[Select
  en_entidad.NROCTA As en_entidad_NROCTA,
  vt_movimiento.RAZONS As vt_movimiento_RAZONS,
  vt_movimiento.ID As vt_movimiento_ID,
  vt_movimiento.FCHMOV As vt_movimiento_FCHMOV,
