		<queryString>
			<![CDATA[Select
  gr_condicion_iva.CODIGO,
  gr_condicion_iva.DESCRP,
  IfNull((Select  Sum(IfNull(vt_movimiento_item.IMPHAB, 0) - IfNull(vt_movimiento_item.IMPDEB,0))
  From
    vt_movimiento Inner Join
    vt_movimiento_item On vt_movimiento.ID = vt_movimiento_item.ID_MCAB
    Inner Join
    gr_comprobante On vt_movimiento.MODCOM = gr_comprobante.MODCOM And
      vt_movimiento.CODCOM = gr_comprobante.CODCOM
  Where vt_movimiento_item.TIPCPT = 'A'
And vt_movimiento_item.CODCPT = 'V000'
And gr_comprobante.SUBDIA = 'S'
And vt_movimiento.CNDIVA = gr_condicion_iva.CODIGO
AND vt_movimiento.FCHMOV BETWEEN $P{FCHDES} AND $P{FCHHAS}
AND gr_comprobante.COMINT = 'N'
AND IF(LENGTH($P{UNINEG}) > 0, vt_movimiento.UNINEG = $P{UNINEG},1=1)
AND IF(LENGTH($P{CODSUC}) > 0, vt_movimiento.CODSUC = $P{CODSUC},1=1)
AND IF(LENGTH($P{CODPRV}) > 0, vt_movimiento.CODPRO = $P{CODPRV},1=1)
AND vt_movimiento.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO})
  Group By
    vt_movimiento.CNDIVA), 0) As GRAVADO_0000,
  IfNull((Select  Sum(IfNull(vt_movimiento_item.IMPHAB, 0) - IfNull(vt_movimiento_item.IMPDEB,0))
  From
    vt_movimiento Inner Join
    vt_movimiento_item On vt_movimiento.ID = vt_movimiento_item.ID_MCAB
    Inner Join
    gr_comprobante On vt_movimiento.MODCOM = gr_comprobante.MODCOM And
      vt_movimiento.CODCOM = gr_comprobante.CODCOM
  Where
    vt_movimiento_item.TIPCPT = 'A' And
    vt_movimiento_item.CODCPT = 'V002' And
    gr_comprobante.SUBDIA = 'S' And
    vt_movimiento.CNDIVA = gr_condicion_iva.CODIGO
AND vt_movimiento.FCHMOV BETWEEN $P{FCHDES} AND $P{FCHHAS}
AND gr_comprobante.COMINT = 'N'
AND IF(LENGTH($P{UNINEG}) > 0, vt_movimiento.UNINEG = $P{UNINEG},1=1)
AND IF(LENGTH($P{CODSUC}) > 0, vt_movimiento.CODSUC = $P{CODSUC},1=1)
AND IF(LENGTH($P{CODPRV}) > 0, vt_movimiento.CODPRO = $P{CODPRV},1=1)
AND vt_movimiento.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO})
  Group By
    vt_movimiento.CNDIVA), 0) As GRAVADO_1050,
  IfNull((Select  Sum(IfNull(vt_movimiento_item.IMPHAB, 0) - IfNull(vt_movimiento_item.IMPDEB,0))
  From
    vt_movimiento Inner Join
    vt_movimiento_item On vt_movimiento.ID = vt_movimiento_item.ID_MCAB
    Inner Join
    gr_comprobante On vt_movimiento.MODCOM = gr_comprobante.MODCOM And
      vt_movimiento.CODCOM = gr_comprobante.CODCOM
  Where
    vt_movimiento_item.TIPCPT = 'A' And
    vt_movimiento_item.CODCPT = 'V001' And
    gr_comprobante.SUBDIA = 'S' And
    vt_movimiento.CNDIVA = gr_condicion_iva.CODIGO
AND vt_movimiento.FCHMOV BETWEEN $P{FCHDES} AND $P{FCHHAS}
AND gr_comprobante.COMINT = 'N'
AND IF(LENGTH($P{UNINEG}) > 0, vt_movimiento.UNINEG = $P{UNINEG},1=1)
AND IF(LENGTH($P{CODSUC}) > 0, vt_movimiento.CODSUC = $P{CODSUC},1=1)
AND IF(LENGTH($P{CODPRV}) > 0, vt_movimiento.CODPRO = $P{CODPRV},1=1)
AND vt_movimiento.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO})
  Group By
    vt_movimiento.CNDIVA), 0) As GRAVADO_2100,
  IfNull((Select  Sum(IfNull(vt_movimiento_item.IMPHAB, 0) - IfNull(vt_movimiento_item.IMPDEB,0))
  From
    vt_movimiento Inner Join
    vt_movimiento_item On vt_movimiento.ID = vt_movimiento_item.ID_MCAB
    Inner Join
    gr_comprobante On vt_movimiento.MODCOM = gr_comprobante.MODCOM And
      vt_movimiento.CODCOM = gr_comprobante.CODCOM
  Where
    vt_movimiento_item.TIPCPT = 'P' And
    gr_comprobante.SUBDIA = 'S' And
    vt_movimiento.CNDIVA = gr_condicion_iva.CODIGO
AND vt_movimiento.FCHMOV BETWEEN $P{FCHDES} AND $P{FCHHAS}
AND gr_comprobante.COMINT = 'N'
AND IF(LENGTH($P{UNINEG}) > 0, vt_movimiento.UNINEG = $P{UNINEG},1=1)
AND IF(LENGTH($P{CODSUC}) > 0, vt_movimiento.CODSUC = $P{CODSUC},1=1)
AND IF(LENGTH($P{CODPRV}) > 0, vt_movimiento.CODPRO = $P{CODPRV},1=1)
AND vt_movimiento.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO})
  Group By
    vt_movimiento.CNDIVA), 0) As PERCEPCIONES,
  IfNull((Select  Sum(IfNull(vt_movimiento_item.IMPHAB, 0) - IfNull(vt_movimiento_item.IMPDEB,0))
  From
    vt_movimiento Inner Join
    vt_movimiento_item On vt_movimiento.ID = vt_movimiento_item.ID_MCAB
    Inner Join
    gr_comprobante On vt_movimiento.MODCOM = gr_comprobante.MODCOM And
      vt_movimiento.CODCOM = gr_comprobante.CODCOM
  Where
    vt_movimiento_item.TIPCPT = 'I' And
    vt_movimiento_item.CODCPT = 'IVA001' And
    gr_comprobante.SUBDIA = 'S' And
    vt_movimiento.CNDIVA = gr_condicion_iva.CODIGO
AND vt_movimiento.FCHMOV BETWEEN $P{FCHDES} AND $P{FCHHAS}
AND gr_comprobante.COMINT = 'N'
AND IF(LENGTH($P{UNINEG}) > 0, vt_movimiento.UNINEG = $P{UNINEG},1=1)
AND IF(LENGTH($P{CODSUC}) > 0, vt_movimiento.CODSUC = $P{CODSUC},1=1)
AND IF(LENGTH($P{CODPRV}) > 0, vt_movimiento.CODPRO = $P{CODPRV},1=1)
AND vt_movimiento.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO})
  Group By
    vt_movimiento.CNDIVA), 0) As IVA_2100,
  IfNull((Select  Sum(IfNull(vt_movimiento_item.IMPHAB, 0) - IfNull(vt_movimiento_item.IMPDEB,0))
  From
    vt_movimiento Inner Join
    vt_movimiento_item On vt_movimiento.ID = vt_movimiento_item.ID_MCAB
    Inner Join
    gr_comprobante On vt_movimiento.MODCOM = gr_comprobante.MODCOM And
      vt_movimiento.CODCOM = gr_comprobante.CODCOM
  Where
    vt_movimiento_item.TIPCPT = 'I' And
    vt_movimiento_item.CODCPT = 'IVA002' And
    gr_comprobante.SUBDIA = 'S' And
    vt_movimiento.CNDIVA = gr_condicion_iva.CODIGO
AND vt_movimiento.FCHMOV BETWEEN $P{FCHDES} AND $P{FCHHAS}
AND gr_comprobante.COMINT = 'N'
AND IF(LENGTH($P{UNINEG}) > 0, vt_movimiento.UNINEG = $P{UNINEG},1=1)
AND IF(LENGTH($P{CODSUC}) > 0, vt_movimiento.CODSUC = $P{CODSUC},1=1)
AND IF(LENGTH($P{CODPRV}) > 0, vt_movimiento.CODPRO = $P{CODPRV},1=1)
AND vt_movimiento.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO})
  Group By
    vt_movimiento.CNDIVA), 0) As IVA_1050,
  IfNull((Select  Sum(IfNull(vt_movimiento_item.IMPDEB, 0) - IfNull(vt_movimiento_item.IMPHAB,0))
  From
    vt_movimiento Inner Join
    vt_movimiento_item On vt_movimiento.ID = vt_movimiento_item.ID_MCAB
    Inner Join
    gr_comprobante On vt_movimiento.MODCOM = gr_comprobante.MODCOM And
      vt_movimiento.CODCOM = gr_comprobante.CODCOM
  Where
    vt_movimiento_item.TIPCPT = 'T' And
    gr_comprobante.SUBDIA = 'S' And
    vt_movimiento.CNDIVA = gr_condicion_iva.CODIGO
AND vt_movimiento.FCHMOV BETWEEN $P{FCHDES} AND $P{FCHHAS}
AND gr_comprobante.COMINT = 'N'
AND IF(LENGTH($P{UNINEG}) > 0, vt_movimiento.UNINEG = $P{UNINEG},1=1)
AND IF(LENGTH($P{CODSUC}) > 0, vt_movimiento.CODSUC = $P{CODSUC},1=1)
AND IF(LENGTH($P{CODPRV}) > 0, vt_movimiento.CODPRO = $P{CODPRV},1=1)
AND IF(LENGTH($P{PTOVTA}) > 0, vt_movimiento.SUCURS = $P{PTOVTA},1=1)
AND vt_movimiento.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO})
  Group By
    vt_movimiento.CNDIVA), 0) As TOTAL

From
  gr_condicion_iva gr_condicion_iva

Where
  1 = 1
Group By
  gr_condicion_iva.DESCRP]]>
		</queryString>
