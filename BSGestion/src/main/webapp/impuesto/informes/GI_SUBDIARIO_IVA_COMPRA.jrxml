  pv_movimiento.NRODOC As pv_movimiento_NRODOC,
  pv_movimiento.TIPDOC As pv_movimiento_TIPDOC,
  pv_movimiento.CODLOC As pv_movimiento_CODLOC,
  pv_movimiento.CNDIVA As pv_movimiento_CNDIVA,
  pv_movimiento.MODFOR As pv_movimiento_MODFOR,
  pv_movimiento.CODFOR As pv_movimiento_CODFOR,
  pv_movimiento.SUCURS As pv_movimiento_SUCURS,
  pv_movimiento.SUCORI As pv_movimiento_SUCORI,
  pv_movimiento.NROORI As pv_movimiento_NROORI,
  gr_formulario.DESCRP As gr_formulario_DESCRP,
  gr_formulario.LETRID As gr_formulario_LETRID,
  gr_provincia.DESCRP As gr_provincia_DESCRP,
  gr_condicion_iva.DESCRP As gr_condicion_iva_DESCRP,
  IfNull((Select Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))
  From pv_movimiento_item  Where  pv_movimiento_item.ID_MCAB = pv_movimiento.ID And   pv_movimiento_item.TIPCPT = 'A' And
    pv_movimiento_item.CODCPT = 'C000'), 0) As GRAVADO_0000,

  IfNull((Select Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))
  From    pv_movimiento_item
  Where
    pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
    pv_movimiento_item.TIPCPT = 'A' And
    pv_movimiento_item.CODCPT = 'C002'), 0) As GRAVADO_1050,

  IfNull((Select Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))
  From   pv_movimiento_item
  Where
    pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
    pv_movimiento_item.TIPCPT = 'A' And
    pv_movimiento_item.CODCPT = 'C001'), 0) As GRAVADO_2100,

  IfNull((Select Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))
  From   pv_movimiento_item
  Where
    pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
    pv_movimiento_item.TIPCPT = 'A' And
    pv_movimiento_item.CODCPT = 'C003'), 0) As GRAVADO_2700,

  IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))
  From
    pv_movimiento_item
  Where
    pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
    pv_movimiento_item.TIPCPT = 'I' And
    pv_movimiento_item.CODCPT = 'IVA001'), 0) As IVA_2100,

  IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))
  From pv_movimiento_item
  Where
    pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
    pv_movimiento_item.TIPCPT = 'I' And
    pv_movimiento_item.CODCPT = 'IVA002'), 0) As IVA_1050,

  IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))
  From pv_movimiento_item
  Where
    pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
    pv_movimiento_item.TIPCPT = 'I' And
    pv_movimiento_item.CODCPT = 'IVA003'), 0) As IVA_2700,

IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))  From pv_movimiento_item  Where   pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
pv_movimiento_item.TIPCPT = 'P' And pv_movimiento_item.CODCPT = 'GAN001'), 0) As PERC_GAN001,

IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))  From pv_movimiento_item  Where   pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
pv_movimiento_item.TIPCPT = 'P' And pv_movimiento_item.CODCPT = 'IBR001'), 0) As PERC_IBR001,

IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))  From pv_movimiento_item  Where   pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
pv_movimiento_item.TIPCPT = 'P' And pv_movimiento_item.CODCPT = 'ITC'), 0) As PERC_ITC,

IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))  From pv_movimiento_item  Where   pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
pv_movimiento_item.TIPCPT = 'P' And pv_movimiento_item.CODCPT = 'IVA001'), 0) As PERC_IVA001,

IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))  From pv_movimiento_item  Where   pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
pv_movimiento_item.TIPCPT = 'P' And pv_movimiento_item.CODCPT = 'PP001'), 0) As PERC_PP001,

IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))  From pv_movimiento_item  Where   pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
pv_movimiento_item.TIPCPT = 'P' And pv_movimiento_item.CODCPT = 'TEM001'), 0) As PERC_TEM001,



  IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPHAB, 0) - IfNull(pv_movimiento_item.IMPDEB,0)) From pv_movimiento_item   Where pv_movimiento_item.ID_MCAB = pv_movimiento.ID And
  pv_movimiento_item.TIPCPT = 'T'), 0) As TOTAL,
  gr_tipo_documento.DESCRP As gr_tipo_documento_DESCRP,
  gr_tipo_documento.CODIGO As gr_tipo_documento_CODIGO,
  gr_formulario.CODDGI As gr_formulario_CODDGI
From  en_entidad en_entidad
Inner Join pv_movimiento pv_movimiento On en_entidad.NROCTA = pv_movimiento.NROCTA
Inner Join gr_formulario gr_formulario On pv_movimiento.MODFOR = gr_formulario.MODFOR And gr_formulario.CODFOR = pv_movimiento.CODFOR
Inner Join gr_provincia gr_provincia On pv_movimiento.CODPRO = gr_provincia.CODIGO And  gr_provincia.CODPAI = pv_movimiento.CODPAI
Inner Join gr_condicion_iva gr_condicion_iva On pv_movimiento.CNDIVA = gr_condicion_iva.CODIGO
Inner Join gr_tipo_documento On pv_movimiento.TIPDOC = gr_tipo_documento.CODIGO
Inner Join gr_comprobante On pv_movimiento.MODCOM = gr_comprobante.MODCOM And pv_movimiento.CODCOM = gr_comprobante.CODCOM

Where gr_comprobante.SUBDIA = 'S'
AND pv_movimiento.ID_MREV IS NULL
AND gr_comprobante.COMINT = 'N'
AND pv_movimiento.FCHMOV BETWEEN $P{FCHDES} AND $P{FCHHAS}
AND IF(LENGTH($P{CODSUC}) > 0, pv_movimiento.CODSUC = $P{CODSUC},1=1)
AND IF(LENGTH($P{CODPRV}) > 0, pv_movimiento.CODPRO = $P{CODPRV},1=1)
AND pv_movimiento.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO})
ORDER BY pv_movimiento.FCHMOV, pv_movimiento.FCHEMI, pv_movimiento.NROFOR]]>
