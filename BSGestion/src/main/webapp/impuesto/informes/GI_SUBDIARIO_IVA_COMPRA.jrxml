IfNull((Select  Sum(IfNull(pv_movimiento_item.IMPDEB, 0) - IfNull(pv_movimiento_item.IMPHAB,0))
  From
    pv_movimiento Inner Join
    pv_movimiento_item On pv_movimiento.ID = pv_movimiento_item.ID_MCAB
    Inner Join
    gr_comprobante On pv_movimiento.MODCOM = gr_comprobante.MODCOM And
      pv_movimiento.CODCOM = gr_comprobante.CODCOM
  Where
    pv_movimiento_item.TIPCPT = 'A' And
    pv_movimiento_item.CODCPT = 'C000' And
    gr_comprobante.SUBDIA = 'S' And
    pv_movimiento.CNDIVA = gr_condicion_iva.CODIGO
AND pv_movimiento.ID_MREV IS NULL
AND pv_movimiento.FCHMOV BETWEEN $P{FCHDES} AND $P{FCHHAS}
AND gr_comprobante.COMINT = 'N'
AND IF(LENGTH($P{UNINEG}) > 0, pv_movimiento.UNINEG = $P{UNINEG},1=1)
AND IF(LENGTH($P{CODSUC}) > 0, pv_movimiento.CODSUC = $P{CODSUC},1=1)
AND IF(LENGTH($P{CODPRV}) > 0, pv_movimiento.CODPRO = $P{CODPRV},1=1)
AND pv_movimiento.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO})
  Group By
    pv_movimiento.CNDIVA), 0) As GRAVADO_0000,

