	<subDataset name="detalle" uuid="528f0a9f-a722-41b2-beae-6ff7dfb2bc98">
		<parameter name="ID_MCAB" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     vt_cuenta_corriente.`CUOTAS` AS vt_cuenta_corriente_CUOTAS,
     vt_cuenta_corriente.`FCHMOV` AS vt_cuenta_corriente_FCHMOV,
     vt_cuenta_corriente.`FCHVNC` AS vt_cuenta_corriente_FCHVNC,
     vt_movimiento.`CODFOR` AS vt_movimiento_CODFOR,
     vt_movimiento.`NROFOR` AS vt_movimiento_NROFOR,
     vt_movimiento.`FCHMOV` AS vt_movimiento_FCHMOV,
     vt_movimiento.`SUCURS` AS vt_movimiento_SUCURS,
     vt_cuenta_corriente.`IMPNAC` AS vt_cuenta_corriente_IMPNAC
FROM
     `vt_cuenta_corriente` vt_cuenta_corriente INNER JOIN `vt_movimiento` vt_movimiento ON vt_cuenta_corriente.`ID_APL` = vt_movimiento.`ID`
WHERE
     vt_cuenta_corriente.ID_MOV = $P{ID_MCAB}
ORDER BY
     vt_cuenta_corriente.IMPNAC DESC]]>
		</queryString>
