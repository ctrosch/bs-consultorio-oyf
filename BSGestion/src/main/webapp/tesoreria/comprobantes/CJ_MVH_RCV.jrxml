		<queryString>
			<![CDATA[SELECT
     gr_tipo_documento.`CODIGO` AS gr_tipo_documento_CODIGO,
     gr_tipo_documento.`DESCRP` AS gr_tipo_documento_DESCRP,
     gr_tipo_documento.`CODIGO` AS gr_tipo_documento_CODIGO,
     gr_formulario.`SUCURS` AS gr_formulario_SUCURS,
     gr_condicion_iva.`CODIGO` AS gr_condicion_iva_CODIGO,
     vt_cobrador.`CODIGO` AS vt_cobrador_CODIGO,
     vt_cobrador.`NOMBRE` AS vt_cobrador_NOMBRE,
     en_entidad.`NROCTA` AS en_entidad_NROCTA,
     en_entidad.`FNTSIA` AS en_entidad_FNTSIA,
     en_entidad.`RAZONS` AS en_entidad_RAZONS,
     en_entidad.DIRECC,
     en_entidad.NUMERO,
     gr_formulario.`DESCRP` AS gr_formulario_DESCRP,
     gr_formulario.`LETRID` AS gr_formulario_LETRID,
     en_entidad.`NRODOC` AS gr_formulario_LETRID,
     gr_localidad.`CODIGO` AS gr_codigo_postal_CODIGO,
     gr_localidad.`DESCRP` AS gr_codigo_postal_DESCRP,
     gr_condicion_iva.`DESCRP` AS gr_condicion_iva_DESCRP,
     gr_provincia.`DESCRP` AS gr_provincia_DESCRP,
     cj_movimiento.`ID` AS cj_movimiento_ID,
     cj_movimiento.`MODCOM` AS cj_movimiento_MODCOM,
     cj_movimiento.`CODCOM` AS cj_movimiento_CODCOM,
     cj_movimiento.`ID_MVT` AS cj_movimiento_ID_MVT,
     cj_movimiento.`MODFOR` AS cj_movimiento_MODFOR,
     cj_movimiento.`CODFOR` AS cj_movimiento_CODFOR,
     cj_movimiento.`NROFOR` AS cj_movimiento_NROFOR,
     cj_movimiento.`SUCURS` AS cj_movimiento_PTOVTA,
     cj_movimiento.`FCHMOV` AS cj_movimiento_FCHMOV,
     cj_movimiento.`NROENT` AS cj_movimiento_NROENT,
     cj_movimiento.`NOMENT` AS cj_movimiento_NOMENT,
     cj_movimiento.`OBSERV` AS cj_movimiento_OBSERV,
     cj_movimiento.`COFSEC` AS cj_movimiento_COFSEC,
     cj_movimiento.`COTIZA` AS cj_movimiento_COTIZA,
     cj_movimiento.`COBRAD` AS cj_movimiento_COBRAD,
     cj_movimiento.`ID_REV` AS cj_movimiento_ID_REV,
     cj_movimiento.`MONREG` AS cj_movimiento_MONREG,
     (Select sum(cj_movimiento_item.IMPORT) As IMPORTE_TOTAL  From cj_movimiento_item WHERE cj_movimiento_item.`ID_MCAB` = cj_movimiento.`ID`  and cj_movimiento_item.TIPCPT <> 'Z') as IMPORTE_TOTAL
FROM
     `gr_formulario` gr_formulario INNER JOIN `cj_movimiento` cj_movimiento ON gr_formulario.`MODFOR` = cj_movimiento.`MODFOR`
     AND gr_formulario.`CODFOR` = cj_movimiento.`CODFOR`
     INNER JOIN `en_entidad` en_entidad ON cj_movimiento.`NROENT` = en_entidad.`NROCTA`
     INNER JOIN `vt_cobrador` vt_cobrador ON cj_movimiento.`COBRAD` = vt_cobrador.`CODIGO`
     INNER JOIN `gr_condicion_iva` gr_condicion_iva ON en_entidad.`CNDIVA` = gr_condicion_iva.`CODIGO`
     INNER JOIN `gr_localidad` gr_localidad ON en_entidad.`CODPOS` = gr_localidad.`ID`
     INNER JOIN `gr_tipo_documento` gr_tipo_documento ON en_entidad.`TIPDOC` = gr_tipo_documento.`CODIGO`
     INNER JOIN `gr_provincia` gr_provincia ON gr_localidad.`CODPRO` = gr_provincia.`CODIGO`
     AND gr_provincia.`CODPAI` = gr_localidad.`CODPAI`

WHERE  cj_movimiento.`ID_MREN` = $P{ID_MREN}]]>
		</queryString>
