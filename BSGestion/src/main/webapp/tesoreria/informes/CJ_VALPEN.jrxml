	<queryString>
		<![CDATA[Select
  cj_movimiento_item.NROCTA,
  cj_cuenta.DESCRP,
  cj_movimiento_item.NROINT,
  cj_movimiento_item.CHEQUE,
  cj_movimiento_item.CODBCO,
  cj_movimiento_item.FCHVNC,
  (Sum(cj_movimiento_item.IMPDEB) - Sum(cj_movimiento_item.IMPHAB)) As IMPORTE,
  IfNull(cj_movimiento_item.NOMENT, '') As NOMBRE,
  cj_movimiento_item.DOCFIR As FIRMANTE,
  cj_movimiento_item.FCHCHE,
  cj_movimiento_item.NRODOC,
  cj_movimiento_item.CATEGO,
  cj_banco.DESCRP As DESCRP1,
  ifnull(cj_movimiento_item.NROENT,'') as NROENT ,
  ifnull(cj_movimiento_item.NOMENT,'') as NOMENT
From
  cj_movimiento_item
  Inner Join cj_movimiento On cj_movimiento.ID = cj_movimiento_item.ID_MCAB
  Inner Join cj_banco On cj_movimiento_item.CODBCO = cj_banco.CODIGO,
  cj_cuenta
Where
  cj_movimiento_item.NROCTA = cj_cuenta.CODIGO
And Not (cj_movimiento_item.NROCTA Is Null)
And cj_cuenta.TIPCTA = 2
AND IF(LENGTH($P{CODSUC})>0,cj_movimiento.CODSUC = $P{CODSUC},1=1)
AND IF(LENGTH($P{NROCTA_CJ})>0,cj_movimiento_item.NROCTA = $P{NROCTA_CJ},1=1)
AND (cj_movimiento.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO})
OR cj_cuenta.CODSUC IN(SELECT n.CODSUC FROM sg_usuario_sucursal n inner join sg_usuario u on n.CODUSU = u.ID WHERE u.USUARIO = $P{USUARIO}))
Group By
  cj_movimiento_item.NROCTA,
  cj_cuenta.DESCRP,
  cj_movimiento_item.NROINT,
  cj_movimiento_item.CHEQUE,
  cj_movimiento_item.CODBCO,
  cj_movimiento_item.FCHVNC,
  cj_movimiento_item.DOCFIR,
  cj_movimiento_item.FCHCHE,
  cj_movimiento_item.NRODOC,
  cj_movimiento_item.CATEGO
Having
  (Sum(cj_movimiento_item.IMPDEB) - Sum(cj_movimiento_item.IMPHAB)) <> 0
Order By
  cj_movimiento_item.NROCTA,cj_movimiento_item.FCHVNC,cj_movimiento_item.CHEQUE]]>
	</queryString>
