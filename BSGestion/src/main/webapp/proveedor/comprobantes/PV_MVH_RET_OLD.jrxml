		</parameter>
		<queryString>
			<![CDATA[Select
  con.DESCRIP,
  pv_movimientoAplicado.NROORI As pv_movimientoAplicado_NROFOR,
  pv_movimientoAplicado.CODFOR As pv_movimientoAplicado_CODFOR,
  RIGHT(CONCAT('0000',IFNULL(pv_movimientoAplicado.SUCORI,pv_movimientoAplicado.SUCURS)),4) As pv_movimientoAplicado_SUCURS,
  pv_movimientoAplicado.FCHEMI As pv_movimientoAplicado_FCHMOV,
  pv_movimientoRET.MODFOR As pv_movimientoRET_MODFOR,
  pv_movimientoRET.CODFOR As pv_movimientoRET_CODFOR,
  pv_movimientoRET.NROFOR As pv_movimientoRET_NROFOR,
  pv_cuenta_corriente.IMPNAC As pv_cuenta_corriente_IMPNAC,
  100*pv_cuenta_corriente.IMPRET/con.PORCEN AS BASE_CALCULO,
 con.EXCEDE,
 (pv_cuenta_corriente.IMPNAC) as pv_cuenta_corriente_BASIMP,
  pv_cuenta_corriente.IMPRET As pv_cuenta_corriente_IMPRET,
  100*pv_cuenta_corriente.IMPRET/(pv_cuenta_corriente.IMPNAC) As pv_cuenta_corriente_PORCEN,
  pv_movimientoOP.CODFOR As pv_movimientoOP_CODFOR,
  pv_movimientoOP.NROFOR As pv_movimientoOP_NROFOR,
  RIGHT(CONCAT('0000',IFNULL(pv_movimientoOP.SUCORI,pv_movimientoOP.SUCURS)),4) As pv_movimientoOP_SUCURS,
  pv_movimientoOP.FCHMOV As pv_movimientoOP_FCHMOV
From
  pv_movimiento_item
  Inner Join pv_retencion_concepto con On pv_movimiento_item.TIPRET = con.TIPRET And  pv_movimiento_item.CODRET = con.CODRET
  Inner Join pv_movimiento pv_movimientoRET On pv_movimiento_item.ID_MCAB = pv_movimientoRET.ID
  Inner Join pv_cuenta_corriente On pv_movimientoRET.ID_MPV = pv_cuenta_corriente.ID_MOV
  Inner Join pv_movimiento pv_movimientoOP On pv_cuenta_corriente.ID_MOV = pv_movimientoOP.ID
  Inner Join pv_movimiento pv_movimientoAplicado On pv_cuenta_corriente.ID_APL = pv_movimientoAplicado.ID
where pv_movimiento_item.TIPCPT = 'A'
AND pv_movimiento_item.`ID_MCAB` = $P{ID_MCAB}]]>
		</queryString>
